(function(){var a={init:function(a){this._initVar(a)},_initVar:function(a){if(!this._shouldChange(a))return;this._htData=a;this._$elWrap=$jq("#"+a.viewId);this._$elThumbArea=this._$elWrap.find(".se_og_thumb");this._bindEvent()},_shouldChange:function(a){return se.CONFIG.SERVICE_ID==="blog"&&a.isVideo&&a.thumbnail&&this._isYoutube(a.link)},_isYoutube:function(a){if(a.indexOf("youtube")>=0)return true},_bindEvent:function(){this._$elThumbArea.on("click",this._onClickThumb.bind(this))},_onClickThumb:function(a){if(this._htData.link.indexOf("v=")<0){return}var b=this._htData.link.split("v=")[1].split("&")[0];var c="https://www.youtube.com/embed/"+b;var d=$jq("<iframe src='"+c+"?autoplay=1&autohide=1' frameborder='0' style='position:absolute;top:0;left:0;width:100%;height:100%'/>");this._$elThumbArea.append(d);this._bindEventAfterReplace();return false},_bindEventAfterReplace:function(){this._$elThumbArea.find("iframe").on("load",function(){this.trigger("onLoadOglink",this._htData)}.bind(this))}};se.defineComponent("se.module.v1.Oglink",a);se.defineComponent("se.module.v1.m.Oglink",a)})();
