	var getNowTime = function getNowTime() {
		var tmpDate = new Date();
		return tmpDate.getTime();
	};
	
	var doneSendPartCount = 0;
	var doneSendPartSize = 0;
	
	var setThumbnailRetryCount = 0;
	
	var RETRY_COUNT = 3;
	
	var continueUpload = true;
	
	var closeVideoUploader = function closeVideoUploader() {
		var uploaderBody = document.getElementById("ugc.uploader.mobile.body");
		if (uploaderBody) {
			uploaderBody.parentNode.removeChild(uploaderBody);
		}
		//var fileInput = document.getElementsByClassName("ugcInputFile")[0];
		continueUpload = false;
		//fileInput.value = "";
		
		closeLogics();
		
		closingUploadCallback();
	};
	
	var getDateString = function getDateString(targetDate) {
		if (targetDate) {
			var year = targetDate.getFullYear();
			var month = targetDate.getMonth() + 1;
			var date = targetDate.getDate();
			var hour = targetDate.getHours();
			var minute = targetDate.getMinutes();
			var second = targetDate.getSeconds();
			return year + "년 " + month + "월 " + date + "일 " + hour + "시 " + minute + "분 " + second + "초";
		}
		return null;
	};
	
	var callbackUploadedInfo = function callbackUploadedInfo(selectedThumbnail) {
		//callbackFunction
		var tmpSvcDomain = properties.serviceDomain;
		if (serviceDomain) {
			tmpSvcDomain = serviceDomain;
		}
		var tagTemplate = "<iframe src='" + tmpSvcDomain + "/view/ugcPlayer.nhn?vid=" + properties.videoId + "&inKey=%INKEY%' width='%WIDTH%' height='%HEIGHT%' frameborder='0'></iframe>";
		if (properties.callbackFunction != null) {
			var resultValue = {"code": "OK","result": {"videoId": properties.videoId, "type":"video", "vendor":"ugc","playTime": properties.playTime,"fileSize": properties.fileSize,"thumbnailURL": selectedThumbnail,"videoTemplate": tagTemplate}};
			var tmpResult = properties.callbackFunction + "(" + JSON.stringify(resultValue).replace(/"/ig, "\\\"") + ");";
			tmpResult = tmpResult.replace(/\\\"/ig, "\"");
			if (typeof properties.callbackFunction == "string") {
				eval(tmpResult);
			} else {
				properties.callbackFunction(resultValue);
			}
		}
	};
	
	var clearUploadedFileData = function clearUploadedFileData() {
		delete properties.uploaderInfo[properties.fileUID];
		setLocalData("uploaderInfo", JSON.stringify(properties.uploaderInfo));
	};
	
	var changeClickSetThumb = function changeLoadingSetThumb() {
		var thumbLoading = document.getElementById("ugc.uploader.mobile.selectThumbLoading");
		var thumbOk = document.getElementById("ugc.uploader.mobile.selectThumbButton");
		
		thumbLoading.style.display = "none";
		thumbOk.style.display = "block";
	};
	
	var changeLoadingSetThumb = function changeLoadingSetThumb() {
		var thumbLoading = document.getElementById("ugc.uploader.mobile.selectThumbLoading");
		var thumbOk = document.getElementById("ugc.uploader.mobile.selectThumbButton");
		
		thumbLoading.style.display = "block";
		thumbOk.style.display = "none";
	};
	
	var retrySetThumbnail = 0;
	var responseSetThumbnail = function responseSetThumbnail(result) {
		//		console.debug("responseSetThumbnail");
		if (result.resultCode == true) {
			//			console.debug("responseSetThumbnail success");
			//thumbnailSmallUrl
			callbackUploadedInfo(result.data.thumbnailSmallUrl);
			
			closeVideoUploader();
		} else {
			retrySetThumbnail++;
			if (retrySetThumbnail < RETRY_COUNT) {
				//				console.debug("retry SETTHUMBNAIL");
				setThumbnail();
			} else {
				alert(naver.ugc.mobile.uploader.message.CANNOT_COMPLATE_NETWORK);
				changeClickSetThumb();
			}
		}
	};
	
	var setThumbnail = function setThumbnail() {
		//		console.debug("setThumbnail");
		if (properties.selectedThumbnail) {
			var url = properties.uploaderDomain + "/upload/mobile/setThumbnail.nhn";
			var param = "sid=" + properties.serviceId 
			+ "&rk=" + properties.randKey 
			+ "&li=" + escape(properties.selectedThumbnail);
			changeLoadingSetThumb();
			sendRequest(url, param, responseSetThumbnail);
		} else {
			alert(naver.ugc.mobile.uploader.message.PLEASE_SELECT_THUMBNAIL);
		}
	};
	
	var clickThumbnail = function clickThumbnail(itemIndex, thumbUrl) {
		properties.selectedThumbnail = thumbUrl;
		document.getElementById("ugc.uploader.mobile.selectedThumb").setAttribute("src", thumbUrl);
		var thumbNodes = document.getElementById("ugc.uploader.mobile.thumbnailList").getElementsByTagName("li");
		for (var i = 0; i < thumbNodes.length; i++) {
			if (i == itemIndex) {
				thumbNodes[i].className = "on";
			} else {
				thumbNodes[i].className = "";
			}
		}
	};
	
	var getThumbItem = function getThumbItem(itemIndex, thumbUrl) {
		var li = document.createElement("li");
		var a = document.createElement("a");
		var img = document.createElement("img");
		img.setAttribute("src", thumbUrl);
		img.setAttribute("width", 50);
		img.setAttribute("height", 50);
		addEvent("click", img, function() {
			clickThumbnail(itemIndex, thumbUrl);
		});
		a.appendChild(img);
		li.appendChild(a);
		return li;
	};
	
	var showThumbnailList = function showThumbnailList(thumbList) {
		var thumbListElement = document.getElementById("ugc.uploader.mobile.thumbnailList");
		for (var i = 0; i < thumbList.length; i++) {
			thumbListElement.appendChild(getThumbItem(i, thumbList[i]));
		}
		addEvent("click", document.getElementById("ugc.uploader.mobile.selectThumbButton"), setThumbnail);
		clickThumbnail(2, thumbList[2]);
		document.getElementById("ugc.uploader.mobile.extractingThumbPane").style["display"] = "none";
		document.getElementById("ugc.uploader.mobile.selectThumbPane").style["display"] = "block";
	};
	
	
	var retryGetVideoInfo = 0;
	var responseGetVideoInfo = function responseGetVideoInfo(result) {
		if (result.resultCode == true) {
			properties.videoId = result.data.videoId;
			properties.playTime = result.data.playTime;
			showThumbnailList(result.data.thumbnailList);
		} else {
			retryGetVideoInfo++;
			if (retryGetVideoInfo < RETRY_COUNT) {
				getVideoInfo();
			} else {
				alert(naver.ugc.mobile.uploader.message.CANNOT_UPLOAD_NETWORK);
				closeVideoUploader();
			}
		}
	};

	//
	var getVideoInfo = function getVideoInfo() {
		if (continueUpload == true) {
			var url = properties.uploaderDomain + "/upload/mobile/getVideoInfo.nhn";
			var param = "sid=" + properties.serviceId 
			+ "&rk=" + properties.randKey;
			sendRequest(url, param, responseGetVideoInfo, 150000);
		}
	};
	
	var retryClosePartFile = 0;
	var responseClosePartFile = function responseClosePartFile(result) {
		if (result.resultCode == true) {
			getVideoInfo();
			clearUploadedFileData();
		} else {
			retryClosePartFile++;
			if (retryClosePartFile < RETRY_COUNT) {
				closePartFile();
			} else {
				alert(naver.ugc.mobile.uploader.message.CANNOT_UPLOAD_NETWORK);
				closeVideoUploader();
			}
		}
	};
	
	var closePartFile = function closePartFile() {
		if (continueUpload == true) {
			var url = properties.uploaderDomain + "/upload/mobile/closePartFile.nhn";
			var param = "sid=" + properties.serviceId + "&fn=" 
			+ properties.fileName + "&hv=" + properties.hashValue 
			+ "&rk=" + properties.randKey + "&dp=" + properties.datePath;
			document.getElementById("ugc.uploader.mobile.uploadingPane").style["display"] = "none";
			
			var extThumbPane = document.getElementById("ugc.uploader.mobile.extractingThumbPane");
			extThumbPane.style["display"] = "block";
			sendRequest(url, param, responseClosePartFile);
		}
	};
	
	var updateUploadedInfo = function updateUploadedInfo(doneCount, bps) {
		var donePercent = Math.round(doneCount / properties.totalCount * 100);
		if (document.getElementById("ugc.uploader.mobile.uploadedPercent")) {
			document.getElementById("ugc.uploader.mobile.uploadedPercent").innerHTML = donePercent + "<em>%</em>";
		}
		if (document.getElementById("ugc.uploader.mobile.uploadedProgress")) {
		document.getElementById("ugc.uploader.mobile.uploadedProgress").style["width"] = donePercent + "%";
		}
		if (document.getElementById("ugc.uploader.mobile.uploadedProgress2")) {
		document.getElementById("ugc.uploader.mobile.uploadedProgress2").style["width"] = donePercent + "%";
		}
		if (document.getElementById("ugc.uploader.mobile.uploadedSize")) {
		document.getElementById("ugc.uploader.mobile.uploadedSize").innerHTML = getFileMBSize(properties.uploadedSize);
		}
		if (document.getElementById("ugc.uploader.mobile.uploadedSize2")) {
		document.getElementById("ugc.uploader.mobile.uploadedSize2").innerHTML = getFileMBSize(properties.uploadedSize);
		}
	};
	
	var saveUploadedPartInfo = function saveUploadedPartInfo() {
		var tmpUploadedInfo = properties.uploaderInfo[properties.fileUID];
		if (tmpUploadedInfo) {
			tmpUploadedInfo.uploadedParts = properties.uploadedParts;
			tmpUploadedInfo.uploadedSize = properties.uploadedSize;
		} else {
			properties.uploaderInfo[properties.fileUID] = {};
			properties.uploaderInfo[properties.fileUID].randKey = properties.randKey;
			properties.uploaderInfo[properties.fileUID].datePath = properties.datePath;
			properties.uploaderInfo[properties.fileUID].hashValue = properties.hashValue;
			properties.uploaderInfo[properties.fileUID].uploadedParts = properties.uploadedParts;
			properties.uploaderInfo[properties.fileUID].bufferSize = properties.bufferSize;
			properties.uploaderInfo[properties.fileUID].fileName = properties.fileName;
			properties.uploaderInfo[properties.fileUID].uploadedSize = properties.uploadedSize;
			properties.uploaderInfo[properties.fileUID].totalPartSize = properties.totalCount;
			properties.uploaderInfo[properties.fileUID].fileSize = properties.fileSize;
		}
		try {
			setLocalData("uploaderInfo", JSON.stringify(properties.uploaderInfo));
		} catch (err) {
			console.log(err);
		}
	};
	
	var closeLogics = function closeLogics() {
		if (sendLogic) {
			sendLogic.end();
		}
		if (queueLogic) {
			queueLogic.end();
		}
	};
	
	var onUploadDone = function onUploadDone(partIndex, wroteSize) {
		properties.uploadedParts[partIndex] = "done";
		saveUploadedPartInfo();
		if (uploadDoneCallCount + alreadyDonePartCount == properties.totalCount) {
			closeLogics();
			closePartFile();
		}
	};
	
	var uploadDoneCallCount = 0;
	var onUploadPartDone = function onUploadPartDone(partLength, wroteSize, partIndex, durationSending) {
		if (partLength > -1) {
			uploadDoneCallCount++;
			properties.uploadedSize += wroteSize;
			var durationSec = durationSending / 1000;
			var bps = 0;
			
			if (durationSec > 0) {
				bps = Math.round(wroteSize / durationSec);
			}
			
			updateUploadedInfo(uploadDoneCallCount + alreadyDonePartCount, bps);
			onUploadDone(partIndex, wroteSize);
		} else {
			alert(naver.ugc.mobile.uploader.message.CANNOT_UPLOAD_NETWORK);
			closeVideoUploader();
		}
	};
	
	var passPartUpload = function passPartUpload(num) {
		updateUploadedInfo(uploadDoneCallCount + alreadyDonePartCount);
		if (uploadDoneCallCount + alreadyDonePartCount == properties.totalCount) {
			onUploadDone();
		}
	};
	
	var alreadyDonePartCount = 0;
	var sendLogic = null;
	var queueLogic = null;
	var sendQueue = null;
	
	var startUploadPartFile = function startUploadPartFile() {
		sendQueue = new naver.ugc.mobile.uploader.FileUploader.DataQueue(properties.queueSize);
		sendLogic = new naver.ugc.mobile.uploader.FileUploader.SendLogic(properties, sendQueue, onUploadPartDone);
		queueLogic = new naver.ugc.mobile.uploader.FileUploader.QueueLogic(properties, sendQueue);
		
		queueLogic.start();
		sendLogic.start();
	};
	
	var uploadFile = function uploadFile() {
		document.getElementById("ugc.uploader.mobile.readyPane").style["display"] = "none";
		document.getElementById("ugc.uploader.mobile.uploadingPane").style["display"] = "block";
		addEvent("click", document.getElementById("ugc.uploader.mobile.cancelUploadButton"), closeUploader);
		properties.timestamp = Number(new Date());
		uploadDoneCallCount = 0;
		alreadyDonePartCount = 0;
		for (var i = 0; i < properties.uploadedParts.length; i++) {
			if (properties.uploadedParts[i] == "done") {
				alreadyDonePartCount++;
				passPartUpload(i);
			}
		}
		if (uploadDoneCallCount + alreadyDonePartCount < properties.totalCount) {
			startUploadPartFile();
		} else {
			closeLogics();
			closePartFile();
		}
	};
	
	var callVideoUploadWindow = function callVideoUploadWindow() {
		var modalGround = document.createElement("div");
		modalGround.id = "ugc.uploader.mobile.body";
		modalGround.style["position"] = "absolute";
		modalGround.style["top"] = "0px";
		modalGround.style["left"] = "0px";
		modalGround.style["z-index"] = 9999;
		modalGround.style["width"] = "100%";
		modalGround.style["height"] = "100%";
		modalGround.innerHTML = getUplaodWindowHTML();
		document.body.insertBefore(modalGround, document.body.childNodes[0]);
	};
	
	var retryOpenFile = 0;
	var responseOpenFile = function responseOpenFile(result) {
		if (result && result.resultCode == true) {
			retryOpenFile = 0;
			properties.randKey = result.data.randKey;
			properties.datePath = result.data.datePath;
			properties.hashValue = result.data.hashValue;
			properties.totalCount = Math.ceil(properties.fileSize / properties.bufferSize);
			properties.uploadedParts = [];
			properties.uploadedParts.length = properties.totalCount;
			for (var i = 0; i < properties.totalCount; i++) {
				properties.uploadedParts[i] = "none";
			}
		} else {
			retryOpenFile++;
			if (retryOpenFile < RETRY_COUNT) {
				openFile();
			} else {
				alert(naver.ugc.mobile.uploader.message.CANNOT_UPLOAD_NETWORK);
				closeVideoUploader();
			}
		}
		
		document.getElementById("ugc.uploader.mobile.uploadButton").className += " on";
		addEvent("click", document.getElementById("ugc.uploader.mobile.uploadButton"), uploadFile);
	};
	
	var closeUploader = function closeUploader() {
		if (confirm(naver.ugc.mobile.uploader.message.NOT_COMPLATE)) {
			closeVideoUploader();
		}
	};
	
	var getSizeAndTimeNoticeTxt = function getSizeAndTimeNoticeTxt() {
		var maxFileMBSize = getFileMBSize(properties.maxSize);
		var maxPlayTime = properties.maxPlayTime;
		
		var noticeTxt = naver.ugc.mobile.uploader.string.FILE_DESCRIPTION_3.replace("$1", Math.floor(maxFileMBSize)).replace("$2", Math.floor(maxPlayTime));
		return noticeTxt;
	}
	
	var setKRServiceName = function setKRServiceName(sid, serviceName) {
        if (KRServiceNameSet[sid] == undefined) {
            return serviceName;
        }
        return KRServiceNameSet[sid];
    }
	
	var retryGetEnv = 0;
	var responseGetEnv = function responseGetEnv(result) {
		if (document.getElementById("ugc.uploader.mobile.closeButton")) {
			addEvent("click", document.getElementById("ugc.uploader.mobile.closeButton"), closeUploader);
			if (result.resultCode == true) {
				if (result.data.userId && result.data.userId.length > 1) {
					properties.uploaderDomain = result.data.serviceUploadUrl;
					properties.serviceName = setKRServiceName(properties.serviceId, result.data.serviceName);
					properties.maxPlayTime = result.data.maxPlayTime;
					document.getElementById("ugc.mobile.uploader.serviceName").innerText = properties.serviceName + " - ";
					document.getElementById("ugc.mobile.uploader.sizeAndTimeInfo").innerHTML = getSizeAndTimeNoticeTxt();
					retryGetEnv = 0;
					checkFile();
				} else {
					alert(naver.ugc.mobile.uploader.message.NOT_LOGIN);
					closeVideoUploader();
				}
			} else {
				retryGetEnv++;
				if (retryGetEnv < RETRY_COUNT) {
					getEnv();
				} else {
					alert(naver.ugc.mobile.uploader.message.CANNOT_UPLOAD_NETWORK);
					closeVideoUploader();
				}
			}
		} else {
			window.setTimeout(function() {responseGetEnv(result);}, 500);
		}
	};
	
	var getEnv = function getEnv() {
		var url = properties.uploaderDomain + "/upload/mobile/getEnv.nhn";
		var param = "sid=" + properties.serviceId;
		sendRequest(url, param, responseGetEnv, 20000);
	};
	
	var checkFile = function checkFile() {
		if (fileUID == null) {
			window.setTimeout(function() {
				checkFile();
			}, 300);
		} else {
			if (isCancelledFile() == true) {
				requestCheckPartFile();
			} else {
				openFile();
			}
		}
	};
	
	var responseCheckFile = function responseCheckFile(result) {
		if (result.resultCode == true) {
			if (confirm(naver.ugc.mobile.uploader.message.EXIST_UPLOAD_RETRY)) {
				var uploadedInfo = properties.uploaderInfo[properties.fileUID];
				properties.randKey = uploadedInfo.randKey;
				properties.datePath = uploadedInfo.datePath;
				properties.hashValue = uploadedInfo.hashValue;
				properties.uploadedParts = uploadedInfo.uploadedParts;
				properties.bufferSize = uploadedInfo.bufferSize;
				properties.fileName = uploadedInfo.fileName;
				properties.uploadedSize = uploadedInfo.uploadedSize;
				properties.totalCount = uploadedInfo.totalPartSize;
				
				for (var i = 0; i < properties.uploadedParts.length; i++) {
					if (properties.uploadedParts[i] != "done") {
						properties.uploadedParts[i] = "none";
					}
				}
				uploadFile();
			} else {
				openFile();
			}
		} else {
			openFile();
		}
	};
	
	var requestCheckPartFile = function requestCheckPartFile() {
		var url = properties.uploaderDomain + "/upload/mobile/checkPartFile.nhn";
		var uploadedInfo = properties.uploaderInfo[properties.fileUID];
		var param = "rk=" + uploadedInfo.randKey + "&sid=" + properties.serviceId + "&hv=" + uploadedInfo.hashValue 
		+ "&tps=" + uploadedInfo.totalPartSize + "&fs=" + uploadedInfo.fileSize + "&dp=" + uploadedInfo.datePath 
		+ "&fn=" + uploadedInfo.fileName;
		sendRequest(url, param, responseCheckFile);
	};
	
	var openFile = function openFile() {
		var url = properties.uploaderDomain + "/upload/mobile/openPartFile.nhn";
		var param = "sid=" + properties.serviceId + "&fn=" 
		+ properties.fileName + "&fs=" + properties.fileSize;
		sendRequest(url, param, responseOpenFile);
	};
	
	var sendRequest = function sendRequest(targetUrl, targetParam, callbackFunction, timeout) {
		xhr = new XMLHttpRequest();
		xhr.open('GET', targetUrl + "?" + targetParam, true);
		xhr.setRequestHeader("Accept", "*/*");
		
		//if (timeout) {
		//	xhr.timeout = timeout;
		//}
		
		xhr.ontimeout = function (err) {
			recievedVideoInfoJson = {
					"resultCode": false,
					"data": {
						"errorCode": "-3",
						"errorMessage": "time out : " + err
					}
				};
			callbackFunction(recievedVideoInfoJson);
		};
		
		xhr.onreadystatechange = function() {
			if (xhr.readyState == 4) {
				var recievedVideoInfoJson;
				if (xhr.status == 200) {
					var response = xhr.responseText;
					try {
						recievedVideoInfoJson = JSON.parse(response);
					} catch (err) {
						recievedVideoInfoJson = {
							"resultCode": false,
							"data": {
								"errorCode": "-1",
								"errorMessage": "javascript exception : " + err
							}
						};
					}
				} else {
					recievedVideoInfoJson = {
						"resultCode": false,
						"data": {
							"errorCode": "-2",
							"errorMessage": "http error : " + xhr.status
						}
					};
				}
				callbackFunction(recievedVideoInfoJson);
			}
			xhr.close;
		};
		xhr.withCredentials = "true";
		xhr.send();
		var runCustomTimeout = function runCustomTimeout() {
			//alert("custom timeout : " + xhr.readyState);
			xhr.aborted = true;
			xhr.abort();
		};
		if (timeout) {
			window.setTimeout(function() {runCustomTimeout();}, timeout);
		}
	};
	
	var isCancelledFile = function isCancelledFile() {
		if (properties.uploaderInfo[properties.fileUID]) {
			return true;
		}
		return false;
	};
	
//	var DEFAULT_UPLOADABLE_SIZE = 52428800; // 50MB
//	var DEFAULT_UPLOADABLE_SIZE = 104857600; // 100MB
	var DEFAULT_UPLOADABLE_SIZE = 157286400; // 150MB
	
	properties.uploaderInfo = {};
	
	var getUploaderInfo = function getUploaderInfo() {
		var tmpInfo = getLocalData("uploaderInfo");
		if (tmpInfo) {
			properties.uploaderInfo = JSON.parse(tmpInfo);
		}
	};
	
	var init = function init() {
		//		console.debug("init");
		var fileUploadButton = document.getElementsByClassName("ugcUploadButton")[0];
		getUploaderInfo();
		continueUpload = true;
		if ((targetFile && targetFile.type.indexOf("video") > -1) || targetFile.type == '') {
			var maxSize = DEFAULT_UPLOADABLE_SIZE;
			if (fileUploadButton.getAttribute("maxsize")) {
				maxSize = parseInt(fileUploadButton.getAttribute("maxsize"));
			}
			if (targetFile.size > maxSize) {
				window.setTimeout(function() {alert(Math.floor(getFileMBSize(maxSize)) + naver.ugc.mobile.uploader.message.CANNOT_FILE_SIZE);},100);
				return false;
			} else if (targetFile.size < 1) {
				window.setTimeout(function() {alert(naver.ugc.mobile.uploader.message.CANNOT_GET_FILE_SIZE);},100);
				return false;
			}
			properties.maxSize = maxSize;
			properties.file = targetFile;
			properties.fileSize = targetFile.size;
			properties.fileName = targetFile.name;
			if (properties.file.mozSlice) {
				properties.file.slice = properties.file.mozSlice;
			}
			if (properties.file.webkitSlice) {
				properties.file.slice = properties.file.webkitSlice;
			}
			if (uploaderDomain) {
				properties.uploaderDomain = uploaderDomain;
			}
			getFileUID();
			getEnv();
			return true;
		} else {
			if (targetFile) {
				window.setTimeout(function() {alert(naver.ugc.mobile.uploader.message.CAN_UPLOAD_ONLY);}, 100);
				closeVideoUploader();
			}
		}
		return false;
	};
	
	var doneCheckUID = false;
	
	var fileUID = null;
	
	var getFileUID = function getFileUID() {
		var makingUIDChunk = 20;
		var readedCount = 0;
		var chunkBlobList = [];
		chunkBlobList.length = makingUIDChunk;
		if (fileUID == null) {
			var fileSeperateGap = Math.round(properties.fileSize / makingUIDChunk);
			var readFileUIDChunk = function readFileUIDChunk(index, slicedBlob) {
				var fReader = new FileReader();
				fReader.onload = function(readedResult) {
					readedCount++;
					chunkBlobList[index] = readedResult.target.result.substring(readedResult.target.result.indexOf(",") + 1);
					if (readedCount + 1 > makingUIDChunk) {
						fileUID = encodeURIComponent(chunkBlobList.join(""));
						properties.fileUID = fileUID + "_" + properties.serviceId;
						doneCheckUID = true;
					}
				};
				fReader.readAsDataURL(slicedBlob);
			};
			for (var i = 0; i < makingUIDChunk; i++) {
				var startIndex = i * fileSeperateGap;
				var blob = properties.file.slice(startIndex, startIndex + 3);
				readFileUIDChunk(i, blob);
			}
		}
	};
	
	if (init()) {
		callVideoUploadWindow();
	} else {
		closeVideoUploader();
	}
};

naver.ugc.mobile.uploader.FileUploader.DataQueue = function DataQueue(size) {
	var _public = {};
	var queue = [];
	var uploadedQueue = [];
	var queueSize = 1;
	var holding = false;
	_public.isEmpty = function isEmpty() {
		if ((queue.length < queueSize) && uploadedQueue.length < queueSize && holding == false) {
			return true;
		}
		return false;
	};
	_public.add = function add(ele) {
		holding = true;
		if (ele) {
			queue.push(ele);
			uploadedQueue.push("sending");
		}
		holding = false;
	};
	_public.poll = function poll() {
		var retValue = null;
		if (queue.length > 0) {
			retValue = queue.shift();
		}
		return retValue;
	};
	_public.doneUpload = function doneUpload() {
		uploadedQueue.shift();
	};
	_public.size = function size() {
		return queue.length;
	};
	queueSize = size;
	return _public;
};

naver.ugc.mobile.uploader.FileUploader.QueueItem = function() {
	var ret = {
		"partNum": -1,
		"totalCount": -1,
		"startIndex": -1,
		"endIndex": -1,
		"partLength": -1};
	return ret;
};

naver.ugc.mobile.uploader.FileUploader.SendLogic = function SendLogic(properties, sendQueue, uploadCallbackFunc) {
	var continueSending = false;
	_public = {};
	
	_public.start = function start() {
		continueSending = true;
		pollQueueItem();
	};
	
	_public.end = function end() {
		continueSending = false;
		for (var numIdx in sendingBuffer) {
			sendingBuffer[numIdx].aborted = true;
			sendingBuffer[numIdx].abort();
		}
	};
	
	var FAIL_COUNT = 10;
	
	var retryFailCount = 0;
	
	var failedSendPart = false;
	
	var doFailPartUpload = function doFailPartUpload(num) {
		retryFailCount++;
		properties.uploadedParts[num - 1] = "none";
		if (retryFailCount > FAIL_COUNT && failedSendPart == false) {
			failedSendPart = true;
			uploadCallbackFunc(-1);
		}
	};
	
	var donePartUpload = function donePartUpload(request, partLength, num, sendingStartTime) {
		if (request.readyState == 4) {
			if (request.status == 200) {
				try {
					delete sendingBuffer[num]; 
					var responseJson = JSON.parse(request.responseText);
					if (responseJson.resultCode == true && responseJson.data.randKey == properties.randKey) {
						var sendingEndTime = getNowTime();
						properties.uploadedParts[num - 1] = "done";
						sendQueue.doneUpload();
						uploadCallbackFunc(partLength, Number(responseJson.data.wroteSize), num - 1, sendingEndTime - sendingStartTime);
					} else {
						properties.uploadedParts[num - 1] = "none";
					}
				} catch (err) {
					doFailPartUpload(num);
				}
			} else {
				doFailPartUpload(num);
			}
		}
		request.close;
	};
	var getNowTime = function getNowTime() {
		var tmpDate = new Date();
		return tmpDate.getTime();
	};
	
	var sendingBuffer = {};
	
	var sendPart = function sendPart(num, totalCount, partLength, fileSize, fileName, targetData) {
		var xhr = new XMLHttpRequest();
		var sendingStartTime = getNowTime();
		xhr.open('POST', properties.uploaderDomain 
		+ '/upload/mobile/uploadPartFile.nhn?' 
		+ "rk=" + properties.randKey + "&sid=" + properties.serviceId 
		+ "&pl=" + partLength + "&pn=" + num + "&hv=" + properties.hashValue 
		+ "&tps=" + totalCount + "&fs=" + fileSize + "&fn=" + fileName 
		+ "&dp=" + properties.datePath, true);
		xhr.setRequestHeader("Accept", "*/*");
		xhr.setRequestHeader("Content-Type", 
		"application/octet-stream; charset=UTF-8");
		xhr.onreadystatechange = function() {
			donePartUpload(xhr, partLength, num, sendingStartTime);
		};
		xhr.withCredentials = "true";
		sendPartStartTime = getNowTime();
		sendingBuffer[num] = xhr;
		xhr.send(targetData.target.result);
		var runCustomTimeout = function runCustomTimeout() {
			if (xhr.readyState < 4) {
				xhr.aborted = true;
				xhr.abort();
			}
		};
		window.setTimeout(function() {runCustomTimeout();}, 120000);
	};
	
	var readPart = function readPart(num, totalCount, partLength, fileSize, fileName, targetBlob) {
		var fReader = new FileReader();
		fReader.onload = function(readedResult) {
			sendPart(num, totalCount, partLength, fileSize, fileName, readedResult);
		};
		fReader.readAsArrayBuffer(targetBlob);
	};
	
	var sliceFile = function sliceFile(queueItem) {
		var startIndex = queueItem.startIndex;
		var endIndex = queueItem.endIndex;
		
		if (properties.fileSize < properties.bufferSize) {
			endIndex = properties.fileSize;
		}
		
		if (endIndex <= properties.fileSize) {
			var blob = properties.file.slice(startIndex, endIndex);
			readPart(queueItem.partNum, properties.totalCount, properties.bufferSize, properties.fileSize, properties.file.name, blob);
		}
	};
	
	var pollQueueItem = function pollQueueItem() {
		if (continueSending != false || sendQueue.isEmpty() != true) {
			var queueItem = sendQueue.poll();
			
			if (queueItem != null) {
				sliceFile(queueItem);
			}
			window.setTimeout(function() {
				pollQueueItem();
			}, 1000);
		}
	};
	
	return _public;
};

naver.ugc.mobile.uploader.FileUploader.QueueLogic = function SendLogic(properties, sendQueue) {
	var continueSending = false;
	_public = {};
	
	_public.start = function start() {
		continueSending = true;
		queuingPartFile();
	};
	
	_public.end = function end() {
		continueSending = false;
	};
	
	var insertQueue = function insertQueue(partIndex) {
		var startIndex = partIndex * properties.bufferSize;
		var endIndex = startIndex + properties.bufferSize;
		
		if (properties.fileSize < endIndex) {
			endIndex = properties.fileSize;
		}
		
		if (endIndex <= properties.fileSize) {
			var queueItem = new naver.ugc.mobile.uploader.FileUploader.QueueItem();
			queueItem.partNum = partIndex + 1;
			queueItem.startIndex = startIndex;
			queueItem.endIndex = endIndex;
			queueItem.partLength = endIndex - startIndex;
			sendQueue.add(queueItem);
			properties.uploadedParts[partIndex] = "queuing";
		}
	};
	
	var uploadedAll = false;
	_public.completeUploadedAll = function completeUploadedAll() {
		return uploadedAll;
	};
	
	var queuingPartFile = function queuingPartFile() {
		if (continueSending == true) {
			if (sendQueue.size() < properties.queueSize) {
				var doneAllUpload = true;
				for (var i = 0; i < properties.uploadedParts.length; i++) {
					//if ((properties.uploadedParts[i] != "done") && (properties.uploadedParts[i] != "queuing") ) {
					if (properties.uploadedParts[i] == "none") {
						doneAllUpload = false;
						insertQueue(i);
						break;
					}
				}
				if (doneAllUpload == false) {
					queuingPartFile();
				} else {
					uploadedAll = true;
				}
			} else {
				window.setTimeout(function() {
					queuingPartFile();
				}, 1000);
			}
		}
	};
	
	return _public;
};

naver.ugc.mobile.uploader.parseUserAgent = function parseUserAgent() {
	var result = {"os":{"name":null, "version":0, "minorVersion":0, "vendor":null, "alias":null, "device":null}
	,"engine":{"name":null, "version":0, "minorVersion":0}
	,"browser":{"name":null, "version":0, "minorVersion":0}
	,"inApp":{"isNaverApp":false, "checkNum":null, "version":0, "minorVersion":0 }
	};
		
	var getNaverApp = function getNaverApp() {
		var inAppRegExp = new RegExp("NAVER\\(inapp;.* (\\d+);.* (\\d+)\\.*(\\d+)*\\.*(\\d+)*","i");
		var resultArray = inAppRegExp.exec(navigator.userAgent);
		if (resultArray && resultArray.length > 2) {
			result.inApp.isNaverApp = true;
			result.inApp.checkNum = parseInt(resultArray[1]);
			result.inApp.version = parseInt(resultArray[2]);
			if (resultArray[3]) {
				result.inApp.minorVersion = parseInt(resultArray[3]);
			}
		}
	};
	
	var getWebEngine = function getWebEngine() {
		var engineRegExp = new RegExp(" (AppleWebKit|Gecko|Presto|Trident)\\/(\\d+)\\.*(\\d+)*", "i");
		var resultArray = engineRegExp.exec(navigator.userAgent);
		if (resultArray && resultArray.length > 2) {
			result.engine.name = resultArray[1];
			result.engine.version = parseInt(resultArray[2]);
			if (resultArray[3]) {
				result.engine.minorVersion = parseInt(resultArray[3]);
			}
		}
	};
	
	var getBrowser = function getBrowser() {
		getWebEngine();
		var resultArray = null;
		if (result.engine.name == "Presto") {
			var browserRegExp = new RegExp("^(opera)\\/(\\d+)\\.*(\\d+)*","i");
			resultArray = browserRegExp.exec(navigator.userAgent);
		} else if (result.engine.name == "Gecko") {
			var browserRegExp = new RegExp(" (firefox)\\/(\\d+)\\.*(\\d+)*","i");
			resultArray = browserRegExp.exec(navigator.userAgent);
		} else if (result.engine.name == "AppleWebKit") {
			var browserRegExp = new RegExp(" (chrome|safari)\\/(\\d+)\\.*(\\d+)*","i");
			resultArray = browserRegExp.exec(navigator.userAgent);
		} else if (result.engine.name == "Trident") {
			var browserRegExp = new RegExp("(rv|MSIE)[: ](\\d+)\\.*(\\d+)*","i");
			resultArray = browserRegExp.exec(navigator.userAgent);
			resultArray[1] = "MSIE";
		} else {
			resultArray = [];
			resultArray.length = 4;
			resultArray["unknown","unknown","0","0"];
		}
		
		if (resultArray && resultArray.length > 2) {
			result.browser.name = resultArray[1];
			result.browser.version = parseInt(resultArray[2]);
			if (resultArray[3]) {
				result.browser.minorVersion = parseInt(resultArray[3]);
			}
		}
		getNaverApp();
	};
	
	var init = function init() {
		for (var key in osList) {
			if (navigator.userAgent.match(key) != null) {
				osList[key]();
				break;
			}
		}
		getBrowser();
	};
	
	var getWindowsNT = function getWindowsNT() {
		var winRegex = new RegExp("Windows NT (\\d+).(\\d+)","i");
		var resultArray = navigator.userAgent.match(winRegex);
		if (resultArray && resultArray.length > 2) {
			result.os.name = "Windows";
			result.os.version = parseInt(resultArray[1]);
			result.os.minorVersion = parseInt(resultArray[2]);
			result.os.vendor = "MicroSoft";
			result.os.alias = "WIN";
		}
	};
	
	var getAndroid = function getAndroid() {
		var andRegExp = new RegExp("(Android) (\\d+)[._](\\d+)","i");
		
		var resultArray = andRegExp.exec(navigator.userAgent);
		
		if (resultArray == null) {
			andRegExp = new RegExp("(Android);.*[rv:]+(\\d+)[._](\\d+)","i");
			resultArray = andRegExp.exec(navigator.userAgent);
		}
		
		if (resultArray && resultArray.length > 2) {
			result.os.name = "Android";
			result.os.version = parseInt(resultArray[2]);
			result.os.minorVersion = parseInt(resultArray[3]);
			result.os.vendor = "Google";
			result.os.alias = "AND";
		}
	};
	var getMacOSDevice = function getMacOSDevice() {
		var macRegExp = new RegExp("\\((iPhone|iPad|iPod).* OS (\\d+)[._](\\d+).*like Mac OS X|(Intel Mac OS X) (\\d+)[._](\\d+)","i");
		
		var resultArray = macRegExp.exec(navigator.userAgent);
		
		if (resultArray && resultArray.length > 2 && resultArray[1]) {
			result.os.name = "iOS";
			result.os.version = parseInt(resultArray[2]);
			result.os.minorVersion = parseInt(resultArray[3]);
			result.os.vendor = "Apple";
			result.os.alias = "IOS";
			result.os.device = resultArray[1];
		} else if (resultArray && resultArray.length > 2 && resultArray[4]) {
			result.os.name = "Mac";
			result.os.version = parseInt(resultArray[6]);
			result.os.minorVersion = parseInt(resultArray[7]);
			result.os.vendor = "Apple";
			result.os.alias = "MAC";
			result.os.device = resultArray[5];
		}
	};
	var getLinux = function getLinux() {
		var regExp = new RegExp("\\(X11;.*Linux (i.86|x86_64)","i");
		var resultArray = regExp.exec(navigator.userAgent);
		
		if (resultArray && resultArray.length > 1) {
			result.os.name = "Linux";
			result.os.version = 0;
			result.os.vendor = "";
			result.os.alias = "LX";
		}
	};
	var getJavaApp = function getJavaApp() {
		
	};
	var getBlackBerry = function getBlackBerry() {
		var regExp = new RegExp("BlackBerry (\\d+);","i");
		var resultArray = regExp.exec(navigator.userAgent);
		
		if (resultArray && resultArray.length > 1) {
			result.os.name = "BlackBerry";
			result.os.version = parseInt(resultArray[1]);
			result.os.vendor = "RIM";
			result.os.alias = "BB";
		}
	};
	var getSimulator = function getSimulator() {
		
	};
	var getTizen = function getTizen() {
		
	};
	
	
	var osList = {"Windows NT":getWindowsNT, "Mac OS X":getMacOSDevice, "Android":getAndroid, "Linux":getLinux
			,"J2ME/MIDP":getJavaApp, "BlackBerry":getBlackBerry, "Simulator":getSimulator, "Tizen":getTizen
	};

	init();
	return result;
};