		cssText += ".u_movie_uploader_thmb_img .u_mvu_thmb_lst li a{display:block;position:relative;width:50px;height:50px;font-size:1px;line-height:0}\n";
		cssText += ".u_movie_uploader_thmb_img .u_mvu_thmb_lst li a img{width:50px !important;height:50px !important}\n";
		cssText += ".u_movie_uploader_thmb_img .u_mvu_thmb_lst li:last-child{margin-right:0}\n";
		cssText += ".u_movie_uploader_thmb_img .u_mvu_thmb_lst li.on a:after{position:absolute;top:-1px;right:-1px;bottom:-1px;left:-1px;border:1px solid #00c185;background-color:rgba(255,255,255,.35);content:\"\";z-index:10}\n";
		cssText += ".u_movie_uploader_link{height:170px}\n";
		cssText += ".u_movie_uploader_link .u_movie_uploader_content{height:170px}\n";
		cssText += ".u_movie_uploader_link .u_movie_uploader_article{padding-top:53px;text-align:left}\n";
		cssText += ".u_movie_uploader_url{position:relative;padding:0 10px 0 9px}\n";
		cssText += ".u_movie_uploader_url .u_mvu_url_bar{display:block;padding:7px 10px 6px;background-color:#e4e6e7;-webkit-box-sizing:border-box}\n";
		cssText += ".u_movie_uploader_url .u_mvu_url_bar input{width:100%;height:20px;padding:0;background-color:#e4e6e7;font-size:15px;color:#41474f;border:0;border-radius:0;line-height:20px;-webkit-appearance:none}\n";
		cssText += ".u_movie_uploader_url .u_mvu_url_bar input::-webkit-input-placeholder{color:#707781}\n";
		cssText += ".u_movie_uploader_url .u_mvu_url_txt{display:block;margin-top:8px;padding:0 10px 0;font-size:13px;color:#a7b0bb}\n";
		cssText += ".u_movie_uploader_btset .u_mvu_bt_link{display:block;background-color:#a0a0a0}\n";
		cssText += ".u_movie_uploader_btset .u_mvu_bt_link.on{background-color:#0daf7d}";
		cssText += ".u_movie_uploader_btset .u_mvu_bt_link span{display:inline-block;position:relative;height:45px;padding-left:21px;color:#fff;font-size:18px;line-height:46px}\n";
		cssText += ".u_movie_uploader_btset .u_mvu_bt_link span:before{position:absolute;left:0;top:50%;width:16px;height:16px;margin-top:-8px;background-position:-86px -128px;content:\"\"}\n";
		cssText += ".u_movie_uploader_btset .u_mvu_loading{display:block;height:45px}\n";
		cssText += ".u_movie_uploader_btset .u_mvu_loading img{position:absolute;left:50%;top:50%;width:23px;height:23px;margin:-11px 0 0 -12px}\n";
		cssText += ".u_movie_uploader_info p{display:block;margin-top:18px;font-size:13px;color:#b2b7c3;line-height:16px;text-align:center}\n";
		styleEle.innerHTML = cssText;
		document.getElementsByTagName("head")[0].appendChild(styleEle);
	};
	
	var hasCSS = function hasCSS() {
		var cssSheetsSize = document.styleSheets.length;
		for (var i = 0; i < cssSheetsSize; i++) {
			if (document.styleSheets[i].title == "ugc.uploader.mobile.cssSheets") {
				return true;
			}
		}
		return false;
	};
	
	if (hasCSS() == false) {
		includeDefaultCSS();
	}
};

