		cssText += ".u_movie_uploader_btset .u_mvu_bt_upload.on{background-color:#0daf7d}";
		cssText += ".u_movie_uploader_btset .u_mvu_bt_upload span{display:inline-block;position:relative;height:45px;padding-left:19px;color:#fff;font-size:18px;line-height:46px}\n";
		cssText += ".u_movie_uploader_btset .u_mvu_bt_upload span:before{position:absolute;left:0;top:50%;width:13px;height:16px;margin-top:-7px;background-position:-39px -128px;content:\"\"}\n";
		cssText += ".u_movie_uploader_btset .u_mvu_bt_cancel{display:block;background-color:#0daf7d}\n";
