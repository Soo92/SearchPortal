	var closeVideoUploader = function closeVideoUploader() {
		var uploaderBody = document.getElementById("ugc.uploader.mobile.body");
		if (uploaderBody) {
			uploaderBody.parentNode.removeChild(uploaderBody);
		}
	};
	
	var addEvent = function addEvent(eventName, targetElement, targetFunction) {
		if (targetElement.addEventListener) {
			targetElement.addEventListener(eventName, targetFunction);
		} else {
			targetElement.attachEvent("on" + eventName, targetFunction);
		}
	};
	
	var callLinkWindow = function callLinkWindow() {
		var modalGround = document.createElement("div");
		modalGround.id = "ugc.uploader.mobile.body";
		modalGround.style["position"] = "absolute";
		modalGround.style["top"] = "0px";
		modalGround.style["left"] = "0px";
		modalGround.style["z-index"] = 9999;
		modalGround.style["width"] = "100%";
		modalGround.style["height"] = "100%";
		modalGround.innerHTML = getLinkWindowHTML();
		document.body.insertBefore(modalGround, document.body.childNodes[0]);
	};
	
	var checkLinkInput = function checkLinkInput() {
		var linkInput = document.getElementById("ugc.uploader.mobile.linkURL");
		if (linkInput) {
			return true;
		}
		return false;
	};
	
	var splitURL = function splitURL(url) {
		return url.match(":\/\/(?:www\.)?(.[^/]+)(.*)");
	};
	
	var getYoutubeVideoId = function getYoutubeVideoId(url) {
		if (url.indexOf("youtu.be") > -1) {
			var matched = url.match(/^.*\:\/\/youtu\.be\/([\w-]{11})/);
			if (matched && matched.length > 1) {
				return matched[1];
			} else {
				return null;
			}
		} else if (url.indexOf("www.youtube.com") > -1) {
			var matched = url.match(/^.*\:\/\/www\.youtube\.com\/watch\?v=([\w-]{11})/);
			if (matched && matched.length > 1) {
				return matched[1];
			} else {
				return null;
			}
		}
	};
	
	var linkInfoTemplate = {"code": "OK",
		"result": {
			"videoId": null,
			"type": "link",
			"vendor":null,
			"thumbnailURL": null,
			"videoTemplate": null,
			"playTime":0,
			"fileSize":0
		}
	};
	
	var getYoutubeLinkInfo = function getYoutubeLinkInfo(url) {
		var videoId = getYoutubeVideoId(url);
		linkInfo = linkInfoTemplate;
		linkInfo.result.videoId = videoId;
		linkInfo.result.vendor = "youtube";
		linkInfo.result.thumbnailURL = "//i.ytimg.com/vi/" + videoId + "/hqdefault.jpg";
		linkInfo.result.videoTemplate = "<iframe id='player' type='text/html' width='%WIDTH%' height='%HEIGHT%' src='//www.youtube.com/embed/" + videoId + "' frameborder='0'></iframe>";
		return linkInfo;
	};
	
	var responseYoutubeData = function responseYoutubeData(result, linkInfo) {
		var errMessage = "";
		var errCode = "";
		if (typeof result.status == "number") {
			if (result.status == naver.ugc.mobile.uploader.statics.link.result.COMPLETE) {
				linkInfo.result = result;
				linkInfo.result.serviceId = serviceId;
				// 하위 호환
				if (result.thumbnail) {
					linkInfo.result.thumbnailURL = result.thumbnail;
				}
				linkInfo.code = "OK";
				callbackLinkInfo(linkInfo);
				return;
			} 
			errMessage = naver.ugc.mobile.uploader.statics.link.message[result.status];
		} else {
			errMessage = naver.ugc.mobile.uploader.message.WRONG_LINK_URL
		}
		alert(errMessage);
		changeLinkButton();
		/*if (result.entry) {
			var playTime = result.entry.media$group.yt$duration.seconds;
			linkInfo.result.playTime = playTime;
			callbackLinkInfo(linkInfo);
		} else {
			alert(naver.ugc.mobile.uploader.message.WRONG_LINK_URL);
			changeLinkButton();
		}*/
	};
	
	var loadingYoutubeData = function loadingYoutubeData(linkInfo, url) {
		var youtubeVideoData = serviceDomain + "/link/videoPlayer.nhn";
		var youtubeVideoDataParam =  "svcUrl=" + escape( url ) + "&sid=" + serviceId + "&level=new";
		/*var youtubeVideoData = "//gdata.youtube.com/feeds/api/videos/" + linkInfo.result.videoId;
		var youtubeVideoDataParam =  "v=2&prettyprint=true&alt=json";*/
		sendRequest(youtubeVideoData, youtubeVideoDataParam, function (result) {responseYoutubeData(result, linkInfo); });
	};
	
	var linkTarget = {
		"youtube.com": {"func": getYoutubeLinkInfo,"type": "youtube","loadingFunc":loadingYoutubeData}
		,"youtu.be": {"func": getYoutubeLinkInfo,"type": "youtube","loadingFunc":loadingYoutubeData}
	};
	var makeLinkInfo = function makeLinkInfo(url) {
		var splitedURL = splitURL(url);
		var linkInfo = null;
		if (splitedURL && splitedURL.length > 1) {
			for (var key in linkTarget) {
				if (splitedURL[1].match(key)) {
					linkInfo = linkTarget[key].func(url);
					if (linkInfo != null) {
						linkInfo.load = linkTarget[key].loadingFunc;
					}
					break;
				}
			}
		}
		return linkInfo;
	};
	
	var callbackLinkInfo = function callbackLinkInfo(linkInfo) {
		if (linkInfo) {
			if (typeof callbackFunction == "string") {
				var tmpResult = callbackFunction + "(" + JSON.stringify(linkInfo).replace(/"/ig, "\\\"") + ");";
				tmpResult = tmpResult.replace(/\\\"/ig, "\"");
				eval(tmpResult);
			} else {
				callbackFunction(linkInfo);
			}
			closeVideoUploader();
		} else {
			alert(naver.ugc.mobile.uploader.message.NOT_SUPPORT_LINK);
			changeLinkButton();
		}
	};
	
	var changeLinkButton = function changeLinkButton() {
		var linkLoading = document.getElementById("ugc.uploader.mobile.linkLoading");
		var linkButton = document.getElementById("ugc.uploader.mobile.linkButton");
		linkLoading.style.display = "none";
		linkButton.style.display = "block";
	};
	
	var changeLoadingLink = function changeLoadingLink() {
		var linkLoading = document.getElementById("ugc.uploader.mobile.linkLoading");
		var linkButton = document.getElementById("ugc.uploader.mobile.linkButton");
		linkLoading.style.display = "block";
		linkButton.style.display = "none";
	};
	//add
	var checkLinkUrl = function checkLinkUrl(url) {
//		matchedArray = url.match(/([\w]+)\.(daum|naver|youtu|youtube)\.(com|net|be)/i);
		matchedArray = url.match(/youtube\.com|youtu\.be/i);
		if (matchedArray && matchedArray.length > 0) {
			return true;
		}
		return false;
	};
	//add
	var getDefaultLinkInfo = function getDefaultLinkInfo() {
		var linkInfo = {"code": "NOT_OK",
				"result": {
					"videoId": null,
					"type": "link",
					"vendor":null,
					"thumbnailURL": null,
					"videoTemplate": null,
					"playTime":0,
					"fileSize":0,
					"source":null
				}
			};
		return linkInfo;
	};
	//add
	var onClickLinkButton = function onClickLinkButton(evt) {
		if (canLink == true) {
			if (checkLinkInput() == true) {
				//loading and callback
				var linkInput = document.getElementById("ugc.uploader.mobile.linkURL");
				if (checkLinkUrl(linkInput.value)) {
					var linkInfo = getDefaultLinkInfo();
					changeLoadingLink();
					loadingYoutubeData(linkInfo, linkInput.value);
				} else {
					alert(naver.ugc.mobile.uploader.message.NOT_SUPPORT_LINK);
				}
			} else {
				alert(naver.ugc.mobile.uploader.message.NOT_SUPPORT_LINK);
			}
		}
	};
	
/*	var onClickLinkButton = function onClickLinkButton(evt) {
		if (canLink == true) {
			if (checkLinkInput() == true) {
				//loading and callback
				var linkInput = document.getElementById("ugc.uploader.mobile.linkURL");
				var linkInfo = makeLinkInfo(linkInput.value);
				if (linkInfo != null) {
					changeLoadingLink();
					if (linkInfo && linkInfo.load) {
						linkInfo.load(linkInfo, linkInput.value);
					} else {
						callbackLinkInfo(linkInfo);
					}
				} else {
					alert(naver.ugc.mobile.uploader.message.NOT_SUPPORT_LINK);
				}
			} else {
				alert(naver.ugc.mobile.uploader.message.NOT_SUPPORT_LINK);
			}
		}
	};*/
	
	var canLink = false;
	var onChangeLinkInput = function onChangeLinkInput(evt) {
		var linkButton = document.getElementById("ugc.uploader.mobile.linkButton");
		if (evt.target.value && evt.target.value.length > 0) {
			if (linkButton.getAttribute("loading") != "done") {
				addEvent("click", linkButton, onClickLinkButton);
			}
			linkButton.className = "u_mvu_bt_link on";
			canLink = true;
		} else {
			canLink = false;
			linkButton.className = "u_mvu_bt_link";
		}
	};
	
	var closeUploader = function closeUploader() {
		if (confirm(naver.ugc.mobile.uploader.message.CLOSE_LINK_WINDOW)) {
			closeVideoUploader();
		}
	};
	
	var init = function init() {
		if (document.getElementById("ugc.uploader.mobile.linkURLPane")) {
			var linkInput = document.getElementById("ugc.uploader.mobile.linkURL");
			addEvent("input", linkInput, onChangeLinkInput);
			addEvent("click", document.getElementById("ugc.uploader.mobile.closeButton"), closeUploader);
		} else {
			window.setTimeout(function() {
				init();
			}, 200);
		}
	};
	
	var sendRequest = function sendRequest(targetUrl, targetParam, callbackFunction) {
		xhr = new XMLHttpRequest();
		xhr.open('GET', targetUrl + "?" + targetParam, true);
		xhr.setRequestHeader("Accept", "*/*");
		xhr.onreadystatechange = function() {
			if (xhr.readyState == 4) {
				var recievedVideoInfoJson;
				if (xhr.status == 200) {
					var response = xhr.responseText;
					try {
						recievedVideoInfoJson = JSON.parse(response);
					} catch (err) {
						recievedVideoInfoJson = {
							"resultCode": false,
							"data": {
								"errorCode": "-1",
								"errorMessage": "javascript exception : " + err
							}
						};
					}
				} else {
					recievedVideoInfoJson = {
						"resultCode": false,
						"data": {
							"errorCode": "-2",
							"errorMessage": "http error : " + xhr.status
						}
					};
				}
				callbackFunction(recievedVideoInfoJson);
			}
			xhr.close;
		};
		xhr.send();
	};
	
	callLinkWindow();
	init();
};

naver.ugc.mobile.uploader.FileUploader = function FileUploader(serviceId, targetFile, callbackFunction, uploaderDomain, closingUploadCallback, serviceDomain) {
	var properties = {
		"file": null,
		"fileSize": 0,
		"fileName": null,
		"serviceId": serviceId,
		"randKey": null,
		"hashValue": null,
		"datePath": null,
		"uploaderDomain": "//uploader.nmv.naver.com",
		"serviceDomain": "//serviceapi.nmv.naver.com",
		"serviceName": "",
		"totalCount": 0,
		"bufferSize": 640 * 1024,
		"timestamp": 0,
		"videoId": null,
		"selectedThumbnail": null,
		"callbackFunction": callbackFunction,
		"queueSize": 2,
		"uploadedSize": 0,
		"maxSize":0,
		"maxPlayTime": 1	//minutes
	};
	
	var KRServiceNameSet = {
        "5":"카페",
        "20005":"카페",
        "4":"지식iN",
        "20004":"지식iN"
    };
	
	var readCount = 0;
	var startIndex = 0;
	var endIndex = properties.bufferSize;
	
	var addEvent = function addEvent(eventName, targetElement, targetFunction) {
		if (targetElement.addEventListener) {
			targetElement.addEventListener(eventName, targetFunction);
		} else {
			targetElement.attachEvent("on" + eventName, targetFunction);
		}
	};
	
	var getFileMBSize = function getFileMBSize(size) {
		var fileMBSize = size / 1024 / 1024;
		return fileMBSize.toFixed(2);
	};
	
	var localStoragePrefix = "ugc.uploader.mobile.data";
	
	var setLocalData = function(name, value) {
		if (localStorage) {
			localStorage.setItem(localStoragePrefix + "." + name, value);
		}
	};
	
	var getLocalData = function(name) {
		if (localStorage) {
			return localStorage.getItem(localStoragePrefix + "." + name);
		} else {
			return null;
		}
	};
	
	var getUplaodWindowHTML = function getUplaodWindowHTML() {
