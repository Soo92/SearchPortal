		cssText += "\n";
		cssText += "/* movie uploader */\n";
		cssText += ".u_movie_uploader{display:table;position:absolute;left:0;top:0;width:100%;height:100%;z-index:100;background:rgba(56,60,69,.97)}\n";
//		cssText += ".u_movie_uploader_wrap{display:table-cell;text-align:center;vertical-align:middle}\n";
		cssText += ".u_movie_uploader_wrap{display:table-cell;text-align:center;padding-top:50px;}\n";
		cssText += ".u_movie_uploader_inner{position:relative;width:300px;margin:0 auto}\n";
		cssText += ".u_movie_uploader_container{position:relative;height:319px;margin:0 auto;background-color:#f2f3f4;-webkit-box-shadow:0 0 8px rgba(0,0,0,.12);box-shadow:0 0 8px rgba(0,0,0,.12)}\n";
		cssText += ".u_movie_uploader_title{position:absolute;left:0;top:0;width:100%;padding:15px 0 0;font-size:21px;font-weight:normal;text-align:center;color:#41474f;line-height:21px}\n";
		cssText += ".u_movie_uploader .u_mvu_bt_close{position:absolute;right:0;top:0;width:30px;height:30px;text-indent:-9999px;background-position:-93px 0}\n";
		cssText += ".u_movie_uploader_content{position:relative;width:100%;height:319px;padding:0 0 45px;-webkit-box-sizing:border-box}\n";
		cssText += ".u_movie_uploader_article{padding-top:92px;text-align:center}\n";
		cssText += ".u_movie_uploader_action{position:relative;width:68px;height:90px;margin:0 auto;background-position:0 0}\n";
