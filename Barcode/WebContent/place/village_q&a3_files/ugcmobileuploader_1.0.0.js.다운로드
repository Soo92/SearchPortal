		cssText += ".u_movie_uploader_thmb_img{width:274px;margin:0 auto}\n";
		cssText += ".u_movie_uploader_thmb_img .u_mvu_thmb_main{display:-webkit-box;width:274px;height:154px;text-align:center;vertical-align:middle;-webkit-box-orient:vertical;-webkit-box-align:center;-webkit-box-pack:center;background-color:#e7e8e9}\n";
