		cssText += ".u_movie_uploader_progress .u_mvu_progress{width:220px;margin:0 auto;padding:5px 0 0}\n";
		cssText += ".u_movie_uploader_progress .u_mvu_progress_bar{overflow:hidden;width:100%;height:2px;background:#cdd1d6;border-radius:2px}\n";
		cssText += ".u_movie_uploader_progress .u_mvu_progress_bar span{display:block;height:2px;background:#11b280}\n";
