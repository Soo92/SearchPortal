naver.ugc.mobile.uploader.loadUploader = function loadUploader() {
	var fileUpload = {"fileInput": null,"uploadButton": null,"serviceId": 0,"callbackFunction": null,"uploadDomain": "//uploader.nmv.naver.com"};
	var externalLink = {"linkButton": null,"serviceId": 0,"callbackFunction": null};
	var uaInfo = null;
	
	var checkFileUploadDevice = function checkFileUploadDevice() {
//		var uaStr = navigator.userAgent;
//		var matchedStr = uaStr.match("(([A-a]ndroid ([4-9]\.|[0-9][0-9]+\.))|((iP(od|hone|ad|od touch)); .*OS ([6-9]|[1-9][0-9]+)_))");
//		if (matchedStr != null) {
//			return true;
//		}
//		return false;
		if (uaInfo) {
			if (uaInfo.os.alias == "AND") {
				if (uaInfo.os.version > 3) {
					if (uaInfo.os.minorVersion == 4 && uaInfo.inApp.isNaverApp == true) {
						return false;
					} else if (uaInfo.engine.name == "Presto" || uaInfo.engine.name == "Gecko") {
						// FF or Opera
						return false;
					}
					return true;
				}
			} else if (uaInfo.os.alias == "IOS") {
				if (uaInfo.os.version > 5) {
					return true;
				}
			}
		}
		return false;
	};
	
	var checkLinkDevice = function checkLinkDevice() {
		if (uaInfo) {
			if (uaInfo.os.alias == "AND") {
				return true;
			} else if (uaInfo.os.alias == "IOS") {
				return true;
			}
		}
		return false;
	};
	
	var init = function init() {
		uaInfo = naver.ugc.mobile.uploader.parseUserAgent();
		var fileUploadButton = document.getElementsByClassName("ugcUploadButton")[0];
		var linkButton = document.getElementsByClassName("ugcLinkButton")[0];
		
		if (fileUploadButton && checkFileUploadDevice() == true) {
			fileUpload.uploadButton = fileUploadButton;
			fileUpload.serviceId = fileUploadButton.getAttribute("serviceId");
			fileUpload.callbackFunction = fileUploadButton.getAttribute("callbackFunction");
			if (fileUploadButton.getAttribute("domain")) {
				fileUpload.uploaderDomain = fileUploadButton.getAttribute("domain");
				fileUpload.serviceDomain = fileUpload.uploaderDomain.replace("uploader.", "serviceapi.");
			}
			addEvent("click", fileUpload.uploadButton, onClickFileInput);
			if (fileUpload.serviceId == "20028" || fileUpload.serviceId == "28") {
				fileUpload.uploadButton.style.display = "inline-block";
			} else {
				fileUpload.uploadButton.style.display = "block";
			}
			createPreInput();
		} else {
			fileUploadButton.style.display = "none";
		}
		if (linkButton && checkLinkDevice() == true) {
			externalLink.linkButton = linkButton;
			externalLink.serviceId = linkButton.getAttribute("serviceId");
			externalLink.callbackFunction = linkButton.getAttribute("callbackFunction");
			if (fileUpload.serviceId == "20028" || fileUpload.serviceId == "28") {
				externalLink.linkButton.style.display = "inline-block";
			} else {
				externalLink.linkButton.style.display = "block";
			}
			externalLink.serviceDomain = "//serviceapi.nmv.naver.com";
			externalLink.uploaderDomain = "//uploader.nmv.naver.com";
			initExternalLink();
		} else {
			if (linkButton) {
				linkButton.style.display = "none";
			}
		}
	};
	
	var removePreInput = function removePreInput() {
		if (fileUpload.fileInput){
			fileUpload.uploadButton.parentNode.removeChild(fileUpload.fileInput);
		}
	};
	
	var createPreInput = function createPreInput() {
		var fileInput = document.createElement("input");
		fileInput.setAttribute("id", "ugcUploaderFileInput_" + (new Date()).getTime());
		fileInput.setAttribute("type", "file");
		fileInput.setAttribute("accept", "video/*");
		fileInput.style.width = "0px";
		fileInput.style.height = "0px";
		fileUpload.uploadButton.parentNode.appendChild(fileInput);
		fileUpload.fileInput = fileInput;
		initFileInput();
	};
	
	function performClick(node) {
		var evt = document.createEvent("MouseEvents");
		evt.initEvent("click", true, false);
		node.dispatchEvent(evt);
	};
	
	var canUsingFileUploader = true;
	
	var useFileUploader = function useFileUploader() {
		canUsingFileUploader = true;
	};
	
	var notUseFileUploader = function notUseFileUploader() {
		canUsingFileUploader = false;
	};
	
	var canUsingExternalLink = true;
	
	var useExternalLink = function useExternalLink() {
		canUsingExternalLink = true;
	};
	
	var notUseExternalLink = function notUseExternalLink() {
		canUsingExternalLink = false;
	};
	
	var onClickFileInput = function onClickFileInput() {
		if (canUsingFileUploader == true) {
			if (checkFileUploadDevice() == true) {
				performClick(fileUpload.fileInput);
			} else {
				alert(naver.ugc.mobile.uploader.message.NOT_SUPPORT_DEVICE);
			}
		}
	};
	
	var openVideoFile = function openVideoFile() {
		if (checkFileUploadDevice() == true) {
			performClick(fileUpload.fileInput);
		} 
	};
	
	var closingUploadCallback = function closingUploadCallback() {
		removePreInput();
		createPreInput();
	};
	
	var onInputFile = function onInputFile() {
		naver.ugc.mobile.uploader.FileUploader(fileUpload.serviceId, 
		fileUpload.fileInput.files[0], fileUpload.callbackFunction, fileUpload.uploaderDomain, closingUploadCallback, fileUpload.serviceDomain);
	};
	
	var onClickLink = function onClickLink() {
		if (canUsingExternalLink == true) {
			if (checkLinkDevice() == true) {
//				naver.ugc.mobile.uploader.ExternalLink(externalLink.serviceId, externalLink.callbackFunction);
				naver.ugc.mobile.uploader.ExternalLink(externalLink.serviceId, externalLink.callbackFunction, externalLink.uploaderDomain, externalLink.serviceDomain);
			} else {
				alert(naver.ugc.mobile.uploader.message.NOT_SUPPORT_DEVICE);
			}
		}
	};
	
	var openExternalLink = function openExternalLink() {
		if (checkLinkDevice() == true) {
//			naver.ugc.mobile.uploader.ExternalLink(externalLink.serviceId, externalLink.callbackFunction);
			naver.ugc.mobile.uploader.ExternalLink(externalLink.serviceId, externalLink.callbackFunction, externalLink.uploaderDomain, externalLink.serviceDomain);
		} 
	};
	
	var initFileInput = function initFileInput() {
		if (!fileUpload.uploadButton.ugcUploader) {
			fileUpload.uploadButton.ugcUploader = {};
		}
		fileUpload.uploadButton.ugcUploader.use = useFileUploader;
		fileUpload.uploadButton.ugcUploader.notUse = notUseFileUploader;
		fileUpload.uploadButton.ugcUploader.open = openVideoFile;
		
		addEvent("change", fileUpload.fileInput, onInputFile);
	};
	
	var initExternalLink = function initExternalLink() {
		if (!externalLink.linkButton.ugcUploader) {
			externalLink.linkButton.ugcUploader = {};
		}
		externalLink.linkButton.ugcUploader.use = useExternalLink;
		externalLink.linkButton.ugcUploader.notUse = notUseExternalLink;
		externalLink.linkButton.ugcUploader.open = openExternalLink;
		
		addEvent("click", externalLink.linkButton, onClickLink);
	};
	
	var addEvent = function addEvent(eventName, targetElement, targetFunction) {
		if (targetElement.addEventListener) {
			targetElement.addEventListener(eventName, targetFunction);
		} else {
			targetElement.attachEvent("on" + eventName, targetFunction);
		}
	};
	naver.ugc.mobile.uploader.loadCSS();
	init();
};

naver.ugc.mobile.uploader.clickUpload = null;

//naver.ugc.mobile.uploader.ExternalLink = function ExternalLink(serviceId, callbackFunction) {
naver.ugc.mobile.uploader.ExternalLink = function ExternalLink(serviceId, callbackFunction, uploaderDomian, serviceDomain) {
