if (typeof window != "undefined" && typeof window.naver == "undefined") {
	window.naver = {};
}

if (typeof window.naver != "undefined" && typeof window.naver.ugc == "undefined") {
	naver.ugc = {};
}

naver.ugc.mobile = {};
naver.ugc.mobile.uploader = {};
naver.ugc.mobile.uploader.canUseUploader = function canUseUploader() {
	return true;
};

naver.ugc.mobile.uploader.loadCSS = function loadCSS() {
	var includeDefaultCSS = function includeDefaultCSS() {
		var styleEle = document.createElement("style");
		styleEle.type = "text/css";
		styleEle.setAttribute("title", "ugc.uploader.mobile.cssSheets");
		var cssText = "";
		cssText += "@charset \"utf-8\";\n";
		cssText += "/* NHN Technology Services KMJ 131008 */\n";
		cssText += "\n";
		cssText += "/* sp load */\n";
		cssText += ".u_mvu_bt_close,\n";
		cssText += ".u_movie_uploader_action,\n";
		cssText += ".u_movie_uploader_action .u_mvu_bt_play,\n";
