"";if(bIsRotate!==true&&!this._bDisableAnimation&&this._oTransition){this._oTransition.queue(this._oElement["aside"],
this._TRANSITION_TIME,{sTaskName:"hideLayer",htStyle:{"right":"-"+wASide.width()+"px"}});this._oElement["aside"].style.position=
"fixed";this._oTransition.start()}else wASide.hide();wDimLayer.hide();this._bisShow=false}else{wASide.show();
eHtml.style["overflow"]="hidden";eHtml.style["height"]=oClientSize.height+"px";eBody.style["overflow"]=
"hidden";eBody.style["height"]=oClientSize.height+"px";wDimLayer.height(oClientSize.height);wDimLayer.show();
if(!this._bDisableAnimation&&this._oTransition){this._oTransition.queue(this._oElement["aside"],this._TRANSITION_TIME,
{sTaskName:"showLayer",htStyle:{"right":"0px"}});this._oTransition.start()}this._bisShow=true}},
_onAfterTransition:function(oEvent){if(oEvent.sTaskName==="hideLayer"){oEvent.element.style.position=
"";$Element(oEvent.element).hide()}},
_profileClick:function(oEvent){var wEl=$Element(oEvent.element);oEvent.stop();if(wEl.hasClass("_asideCloseBtn"))this.toggle();
if(wEl.hasClass("_profileThum")||wEl.hasClass("_myProfile")){this._setStateTohash();location.href="/profile/index.nhn?u\x3d"+
encodeURIComponent(this._sUserKey)}},
_onProfileLoading:function(oResponse){this._hideSlideLoader();var result=oResponse.json();if(result&&
result.isSuccess){var myKinInfo=result.result[0];this._renderProfileArea(myKinInfo.myInfo);this._renderLevupArea(myKinInfo.myInfo.levelupInfo);
this._renderMessageArea(myKinInfo.messageLists);if(this._layerState.isOpen)this.toggle();this._setScroll()}},
_renderProfileArea:function(myInfo){var sUserName=myInfo.userId;if(myInfo.useName)sUserName=myInfo.userName;
else if(myInfo.useNickname)sUserName=myInfo.nickname;var sHtml=this._wTemplate["profile"].process({photoUrl:myInfo.photoUrl,
userName:sUserName,levelName:myInfo.levelName,totalPoint:nhn.Kin.getFormattedNumber(myInfo.totalPoint),
encodedU:myInfo.encodedU});this._wElement["profile"].html(sHtml)},
_renderLevupArea:function(levelupInfo){var sHtml=this._wTemplate["lvup"].process({isMaxLevel:levelupInfo.isMaxLevel,
currentLevelName:levelupInfo.currentLevelName,nextLevelName:levelupInfo.nextLevelName,myPoint:nhn.Kin.getFormattedNumber(levelupInfo.myPoint),
myBestCount:nhn.Kin.getFormattedNumber(levelupInfo.myBestCount),myBestRate:parseFloat(levelupInfo.myBestRate).toFixed(0),
bNeedPoint:levelupInfo.needPoint>0,needPoint:nhn.Kin.getFormattedNumber(levelupInfo.needPoint),bNeedBestCount:levelupInfo.needBestCount>
0,needBestCount:nhn.Kin.getFormattedNumber(levelupInfo.needBestCount),bNeedBestRate:levelupInfo.needBestRate>
0,needBestRate:parseFloat(levelupInfo.needBestRate).toFixed(0)});this._wElement["lvup"].html(sHtml)},
_renderMessageArea:function(messageLists){if(!messageLists||!messageLists.privateMessageList)return;this._currentContentsCount=
messageLists.privateMessageList.listCount;var sHtml=this._wTemplate["privateMsgTpl"].process(messageLists.privateMessageList);
this._wElement["privateMsg"].html(sHtml);this._limitTimestamp=messageLists.privateMessageList.lastTimestamp;
this._limitMessageNo=messageLists.privateMessageList.lastMessageNo;this._wElement["privateMsgMore"]=$Element("privateMsgMore");
this._wElement["privateMsgList"]=$Element("privateMsgList");this._prependHTML(messageLists.realtimeMessageList);
this._prependHTML(messageLists.publicMessageList);this._setMyKinLayerIcon();if(messageLists.privateMessageList.isLastPage)this._wElement["privateMsgMore"].hide()},
_prependHTML:function(aMessageList){var sHtml="";if(aMessageList){for(var i=0;i<aMessageList.length;i++){var message=
aMessageList[i];sHtml+=this._wTemplate["messageMoreTpl"].process(message)}this._wElement["privateMsgList"].html(sHtml+
this._wElement["privateMsgList"].html())}},
_setMyKinLayerIcon:function(){var marks=$$("div.mark",this._oElement["privateMsg"]);for(var i=0;i<marks.length;i++){var wMark=
$Element(marks[i]);var wI=wMark.first();var wA=wMark.parent();if(wI.hasClass("mark_adopt")||wI.hasClass("mark_q_v3")){wI.className("sp_kin ico ico_mark_q_v2");
if(wA.hasClass("shape_l")){wA.removeClass("shape_l");wA.addClass("shape_s")}}else if(wI.hasClass("mark_a_v3")){wI.className("sp_kin ico ico_mark_a_v2");
if(wA.hasClass("shape_l")){wA.removeClass("shape_l");wA.addClass("shape_s")}}else wA.remove(wMark)}},
_privateMsgClick:function(oEvent){var wEl=$Element(oEvent.element);if(wEl.hasClass("_more")){this._readMoreMessages(oEvent);
oEvent.stop();return}try{if(!/_deleteMessage/.test(wEl._element.className)){wEls=wEl.parent(function(v){return v.hasClass("_deleteMessage")},
2);
if(wEls.length>0)wEl=$Element(wEls[0]._element)}if(/_deleteMessage/.test(wEl._element.className)){var nMessageNo=
this._extractMessageNo(wEl._element);this._deleteMessage("private",nMessageNo,wEl);oEvent.stop();return}}catch(e){}this._setStateTohash()},
_setStateTohash:function(){this._oKinHashUtil.setStateToHash({myKin:{isOpen:true}})},
_readMoreMessages:function(oEvent){location.href="/mobile/mykin/myQnaList.nhn?listType\x3dmessageList\x26isForce\x3dY"},
_onClickQuestionBtn:function(oEvent){if(nhn.Kin.checkRos())return false;var oOption={method:"post",timeout:5,
onload:$Fn(this._onPermissionCheck,this).bind(),ontimeout:$Fn(function(){alert(nhn.Kin.oMessage.TIMEOUT)},
this).bind(),
onerror:$Fn(function(){alert(nhn.Kin.oMessage.ERROR)},this).bind()};
var oAjax=new $Ajax(this._QUESTION_WRITABLE_URL,oOption);oAjax.request()},
_onPermissionCheck:function(oResponse){var oJson=oResponse.json();var result=oJson?oJson.result[0]:null;
if(oJson&&(oJson.isSuccess||oJson.errorMsg==="NOT_LOGIN")){this._setStateTohash();location.href="/mobile/qna/question.nhn"}else alert(oJson.errorMsg)},
_deleteMessage:function(sDeleteType,nMessageNo,wEl){if(nhn.Kin.checkRos())return false;var oParam={"type":sDeleteType,
"no":nMessageNo,"ts":(new Date).getTime()};var oOption={method:"post",timeout:5,onload:$Fn(this._onDeleteMessage,
this).bind(wEl),ontimeout:$Fn(function(){alert(nhn.Kin.oMessage.TIMEOUT)},this).bind(),
onerror:$Fn(function(){alert(nhn.Kin.oMessage.ERROR)},this).bind()};
var oAjax=new $Ajax(this._DELETE_MESSAGE_URL,oOption);oAjax.request(oParam)},
_onDeleteMessage:function(wEl,oResponse){var oJson=oResponse.json();if(oJson&&oJson.isSuccess){var aWLi=
wEl.parent(function(el){return el.tag.toLowerCase()==="li"},3);
if(aWLi.length>0)aWLi[0].hide()}else if(oJson.errorMsg)alert(oJson.errorMsg);else this._alertErrorMessage()},
_alertErrorMessage:function(){alert("\uba54\uc2dc\uc9c0 \uc0ad\uc81c\uc5d0 \uc2e4\ud328\ud558\uc600\uc2b5\ub2c8\ub2e4.")},
_extractMessageNo:function(el){if(!el)return"";var sClassName=el.className;if(!sClassName||sClassName.indexOf("_params")<
0)return"";return sClassName.replace(/.*_params\(([^\)]+)\).*/g,"$1")},
_onClickAnswerBtn:function(oEvent){oEvent.stop();this._setStateTohash();location.href="/mobile/qna/index.nhn"},
_setScroll:function(){if(!this._oMyKinScroll){this._oMyKinScroll=new jindo.m.Scroll(this._wElement["aside"],
{bUseHScroll:false,bUseVScroll:true,bUseMomentum:true,nDeceleration:5E-4,nHeight:$Document(document).clientSize().height,
bAutoResize:true,bUseScrollbar:false});this._wElement["aside"].$value().style.zIndex=11010;this._wElement["aside"].child()[0].$value().style.zIndex=
11010}else this._oMyKinScroll.height($Document(document).clientSize().height)},
destroy:function(){this._oElement=this._wElement=this._wTemplate=this._oEvent=null}});
