_onActivate:function(){if(!this._oTouch)this._oTouch=new jindo.m.Touch(this._htWElement["view"].$value(),
{nMoveThreshold:0,nUseDiagonal:0,bVertical:this.bUseH,bHorizental:!this.bUseH,nMomentumDuration:200,nTapThreshold:1,
