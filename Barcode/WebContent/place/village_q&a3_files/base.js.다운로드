if(!p)p=.3;if(!a||a<1){a=1;n=p/4}else n=p/(2*Math.PI)*Math.asin(1/a);return a*Math.pow(2,-10*s)*Math.sin((s-
n)*(2*Math.PI)/p)+1});
jindo.m.Effect.easeInOutElastic=jindo.m.Effect(function(s){var p=0,a=0,n;if(s===0)return 0;if((s=s/(1/
2))==2)return 1;if(!p)p=.3*1.5;if(!a||a<1){a=1;n=p/4}else n=p/(2*Math.PI)*Math.asin(1/a);if(s<1)return-.5*
(a*Math.pow(2,10*(s-=1))*Math.sin((s-n)*(2*Math.PI)/p));return a*Math.pow(2,-10*(s-=1))*Math.sin((s-n)*
(2*Math.PI)/p)*.5+1});
jindo.m.Effect.easeOutBounce=jindo.m.Effect(function(s){if(s<1/2.75)return 7.5625*s*s;else if(s<2/2.75)return 7.5625*
(s-=1.5/2.75)*s+.75;else if(s<2.5/2.75)return 7.5625*(s-=2.25/2.75)*s+.9375;else return 7.5625*(s-=2.625/
