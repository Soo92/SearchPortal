    }, , , , , , , , , , , , , , , , , , , , , , , , , , , , , , , , , , , , , , , , , , , , , , , , function(t, e) {
        var i = Backbone.View.extend({
            SELECTOR_SCROLLER: "._scroller",
            SELECTOR_PREV_BUTTON: "._btn_prev_recommend_album",
            SELECTOR_NEXT_BUTTON: "._btn_next_recommend_album",
            initialize: function() {
                this._assignElement(), this._bBtnActivated = !0, this._hasAllElements() && (this._attachEvent(), 
                this._initCarousel());
            },
            _assignElement: function() {
                this.$body = $(document.body), this.$scroller = this.$el.find(this.SELECTOR_SCROLLER), 
                this.$btnPrev = this.$body.find(this.SELECTOR_PREV_BUTTON), this.$btnNext = this.$body.find(this.SELECTOR_NEXT_BUTTON);
            },
            _hasAllElements: function() {
                var t = !0;
                return (!this.$scroller || this.$scroller.length <= 0) && (t = !1), (!this.$btnPrev || this.$btnPrev.length <= 0) && (t = !1), 
                (!this.$btnNext || this.$btnNext.length <= 0) && (t = !1), t;
            },
            _attachEvent: function() {
                $(window).on("resize", $.proxy(this._updateStatus, this)), this.$btnPrev.on("click", $.proxy(this._onClickPrevAlbum, this)), 
                this.$btnNext.on("click", $.proxy(this._onClickNextAlbum, this));
            },
            _onClickPrevAlbum: function(t) {
                t.preventDefault(), this._oCarousel.movePrev();
            },
            _onClickNextAlbum: function(t) {
                t.preventDefault(), this._oCarousel.moveNext();
            },
            _isWideToScroll: function() {
                return this.$scroller.width() > this.$body.width();
            },
            _toggleButtons: function() {
                var t = "show", e = "show", i = this._oCarousel.getIndex(), o = this._oCarousel.getLastIndex();
                this._bBtnActivated = this._isWideToScroll(), this._bBtnActivated ? (0 === i && (t = "hide"), 
                i === o && (e = "hide")) : (t = "hide", e = "hide"), this.$btnPrev[t](), this.$btnNext[e]();
            },
            _updateStatus: function() {
                this._toggleButtons(), this.$body.trigger("LOAD_IMAGES", this.el);
            },
            _initCarousel: function() {
                this.$scroller.css("position", "relative"), this._oCarousel = new PHOLAR.Carousel(this.$el, {
                    bUseLoadImage: !1
                }), this._oCarousel.on("afterScroll", $.proxy(this._updateStatus, this)), this._updateStatus();
            }
        });
        t.exports = i;
    } ]);
