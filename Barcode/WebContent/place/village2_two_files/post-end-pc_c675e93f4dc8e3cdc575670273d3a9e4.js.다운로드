                    this._getNaverId());
                    break;

                  case this.FACEBOOK_LOGIN_MAPPING_NOT:
                    "true" !== $.cookie(this.COOKIE_KEY_MAPPING_NOTYET) && ($(document.body).trigger("SCROLL_LOCK"), 
                    this.elements.$wrapMappingNotYetLayer.show());
                    break;

                  case this.PROCESSING_MAPPING_COMPLETE:
                    $(document.body).trigger("SCROLL_LOCK"), this.elements.$wrapMappingCompleteLayer.show();
                    break;

                  case this.PROCESSING_MAPPING_MAPPING_FAIL_DUP:
                    this.elements.$wrapMappingDenyLayer.show(), $(document.body).trigger("SCROLL_LOCK");
                }
            },
            _hideComplete: function(t) {
