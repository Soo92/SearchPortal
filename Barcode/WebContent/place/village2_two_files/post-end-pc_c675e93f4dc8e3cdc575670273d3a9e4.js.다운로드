                "click ._btn_go_mapping": "_goMapping",
                "click ._btn_just_close": "_justClose",
                "click ._btn_just_close2": "_justClose2"
            },
            initialize: function() {
                this._assignElement(), this._showLayer(), $.cookie(this.COOKIE_KEY_LAYER_TYPE) && $.removeCookie(this.COOKIE_KEY_LAYER_TYPE);
            },
            _assignElement: function() {
                this.elements = {}, this.elements.$wrapMappingAlreadyLayer = this.$el.find("._wrap_login_noti_layer_already_mapping"), 
                this.elements.$wrapMappingNotYetLayer = this.$el.find("._wrap_login_noti_layer_mapping_not_yet"), 
