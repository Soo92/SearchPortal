                    } else t.code && (t.success === !1 || t.result && 0 !== t.result.resultStatusCode && 2003 !== t.result.resultStatusCode) ? (n.toast(t.result.resultMessage), 
                    e.reject(t)) : ($.removeCookie("sStatusError"), e.resolve(t));
                    a = _.without(a, i.url);
                });
                var s = e.promise();
                return s.abort = $.proxy(o.abort, o), s;
            },
