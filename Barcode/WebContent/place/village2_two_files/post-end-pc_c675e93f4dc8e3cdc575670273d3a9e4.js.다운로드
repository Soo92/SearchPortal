            },
            _duplicatedTagCheck: function() {
                var t = this.$tags.val().split("#");
                t = t.splice(1, t.length);
                for (var e = 0; e < t.length; e++) for (var i = e + 1; i < t.length; i++) t[e] == t[i] && (a.toast("동일한 태그는 사용하실 수 없어요."), 
                this.$tags.focus(), t[i] = "");
                var o = "";
                for (e = 0; e < t.length; e++) "" !== t[e] && (o += "#" + t[e]);
                this.$tags.val(o);
            },
            _overflowTagCheck: function() {
                if (tags = this.$tags.val().split("#"), tags = tags.splice(1, tags.length), tags.length > 3) {
                    for (returnStr = "", i = 0; i < 3; i++) returnStr += "#" + tags[i];
                    this.$tags.val(returnStr), a.toast("태그는 3개까지 가능해요!");
                }
            },
            _submitAlbumCreate: function(t) {
                this.submitUrl = "/api/album/create", this._targetAlbumType = $(t.target).data("albumType");
                var e = this.$body.find("._album_scrap_postkey");
                0 != e.length && (this._targetScrapPostKey = e.val()), this._requestAlbumEdit();
            },
            _submitAlbumUpdate: function(t) {
                this._targetAlbumNo = $(t.target).data("albumNo"), this._targetAlbumType = $(t.target).data("albumType"), 
                this.submitUrl = "/api/album/update/" + this._targetAlbumNo, this._requestAlbumEdit();
            },
            _requestAlbumEdit: function() {
                if (void 0 == this._targetAlbumType || "" == this._targetAlbumType) return !1;
                if (void 0 == this.submitUrl || "" == this.submitUrl) return !1;
                var t = {
                    title: $(this.SELECTOR_ALBUM_TITLE).val(),
                    desc: $(this.SELECTOR_ALBUM_DESC).val(),
                    tags: $(this.SELECTOR_ALBUM_TAGS).val(),
                    albumType: this._targetAlbumType,
                    albumCoverUrl: $(this.SELECTOR_ALBUM_COVER_URL).val(),
                    albumCoverWidth: $(this.SELECTOR_ALBUM_COVER_WIDTH).val(),
                    albumCoverHeight: $(this.SELECTOR_ALBUM_COVER_HEIGHT).val(),
                    albumCoverDominantColor: $(this.SELECTOR_ALBUM_COVER_DOMINANT_COLOR).val()
                };
                void 0 !== this._targetScrapPostKey && (t.postKey = this._targetScrapPostKey);
                var e = n.request({
                    url: this.submitUrl,
                    method: "POST",
                    data: t
                });
                e.done($.proxy(this._onCreateSuccess, this)), e.fail($.proxy(this._onCreateFail, this));
            },
            _onCreateSuccess: function(t) {
                var e = t.result;
                void 0 !== window.history.replaceState && window.history.replaceState(null, null, "/my/album"), 
                window.location.href = "/album/" + e.memberNo + "/" + e.albumNo;
            },
            _onCreateFail: function(t) {
                null !== t && a.toast(t.result.errorMessage);
            },
            _showDeleteAlbumPopup: function(t) {
                this._targetMemberNo = $(t.target).data("memberNo"), this._targetAlbumNo = $(t.target).data("albumNo"), 
                this._targetAlbumType = $(t.target).data("albumType");
                var e = this.$body.find("._ly_delete_album_wrap");
