                e.find("._ly_normal_album").show()), $(document).find(".chk_wrap label").removeClass("checked"), 
                $(document).find('.chk_wrap input[type="checkbox"]').prop("checked", !1), r.activate("._ly_delete_album_wrap", {
                    disableDimmed: !0,
                    disableScrollLock: !0
                });
                var i = $("._ly_delete_album_wrap").find("._ly_popup");
                n.alignCenter(i);
            },
            _submitAlbumDelete: function() {
                var t = this.$body.find("._ly_delete_album_wrap #isDeletePost").prop("checked"), e = n.request({
                    url: s.getConfig("API_REMOVE_ALBUM"),
                    method: "POST",
                    data: {
                        memberNo: this._targetMemberNo,
                        albumNo: this._targetAlbumNo,
                        albumType: this._targetAlbumType,
                        isDeletePost: t
                    }
                });
                e.done($.proxy(this._onDeleteSuccess, this)), e.fail($.proxy(this._onDeleteFail, this));
            },
            _onDeleteSuccess: function() {
                var t = this.$body.find("._delete_album_btn").data("memberNo");
                window.location.href = "/my/" + t + "/profile";
            },
            _onDeleteFail: function(t) {},
            _changeRandomAlbumCover: function() {
                return void 0 !== this.$layer && (this.defaultCoverImgOffset = (this.defaultCoverImgOffset + 1) % this.defaultCoverImg.length, 
                this.selectedAlbumCover = this.defaultCoverImg[this.defaultCoverImgOffset], void this.trigger("ALBUM_COVER_CHANGED"));
            },
            _setCoverUploader: function() {
                this.$cover.fileupload({
                    url: "/api/album/albumCover/create",
                    dataType: "json",
                    add: $.proxy(function(t, e) {
                        var i = e.files[0];
                        return /png|jpe?g|gif/i.test(i.name) ? i.size > 10485760 ? (a.toast("파일 용량은 10MB를 초과할 수 없습니다."), 
                        !1) : void e.submit() : (a.toast("png, jpg, gif 만 가능합니다."), !1);
                    }, this),
                    submit: function() {
                        n.loading(!0);
                    },
                    done: $.proxy(this._onSuccessUpload, this),
                    fail: function() {
                        a.toast("앨범 커버 등록에 실패했습니다."), n.loading(!1);
                    }
                });
            }
        });
        t.exports = l;
