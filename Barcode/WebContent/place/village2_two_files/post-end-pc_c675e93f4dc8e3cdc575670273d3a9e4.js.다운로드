            _checkAlbumTags: function(t) {
                var e = this.$tags.val();
                0 == e.length && "" === e.replace(/^\s+|\s+$/g, "") && this.$tags.val("#");
                var i = /[\{\}\[\]\/?.,;:|\)*~`!^\-+<>@\$%&\\\=\(\'\"]/gi;
                i.test(this.$tags.val()) && (this.$tags.val(this.$tags.val().replace(i, "")), a.toast("특수문자는 태그에 사용하실 수 없어요!"));
                var o = / +/g;
                o.test(this.$tags.val()) && this.$tags.val(this.$tags.val().replace(o, "#"));
                var n = /##/g;
                n.test(this.$tags.val()) && this.$tags.val(this.$tags.val().replace(/#+/g, "#"));
                var s = /# +/g;
                s.test(this.$tags.val()) && this.$tags.val(this.$tags.val().replace(s, "#"));
