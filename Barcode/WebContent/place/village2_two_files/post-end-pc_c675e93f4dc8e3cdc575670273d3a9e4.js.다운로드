            },
            confirm: function(t, e) {
                var i = "confirm";
                if (!t || "undefined" === t || "object" != typeof t) throw new Error("커스텀 " + i + " 레이어팝업 호출시 첫번째 파라미터에 환경설정 옵션이 정의되어야 합니다. (필수)");
                if (!t.title || "string" != typeof t.title || "undefined" === t.title) throw new Error("커스텀 " + i + "의 타이틀 text가 정의되지 않았습니다.");
                if (!t.contents || "string" != typeof t.contents || "undefined" === t.contents) throw new Error("커스텀 " + i + "의 컨텐츠 html이 정의되지 않았습니다.");
                if (t.bottons && "string" != typeof t.bottons) throw new Error("커스텀 " + i + "의 버튼을 변경하려면 html이 정의되어야 합니다.");
                if (!e || "function" != typeof e) throw new Error("커스텀 " + i + " 호출시 두번째 파라미터에 콜백함수가 정의되어야 합니다. (필수)");
                return this.isSystemPopupActivated() ? null : (this.isActivated() && this.deactivateAll(), 
                void this._activateSystemPopup(i, t, e));
            },
            alert: function(t, e) {
                var i = "alert";
                if (!t || "undefined" === t || "object" != typeof t) throw new Error("커스텀 alert 레이어팝업 호출시 첫번째 파라미터에 환경설정 옵션이 정의되어야 합니다. (필수)");
