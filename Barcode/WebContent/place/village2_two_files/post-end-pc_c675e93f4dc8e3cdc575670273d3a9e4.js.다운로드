                    l = "" != l[0] ? l : l.splice(1, l.length);
                    for (var _ = !1, c = 0; c < l.length; c++) {
                        var h = l[c];
                        h.length > 20 && (this.$tags.blur(), h = null != l[c] ? h.substring(0, 20) : h.substring(0, 20) + "#", 
                        _ || (_ = !0, a.toast("각 태그는 20자까지 가능해요!"))), h = "#" + h, r += h;
                    }
                    _ && (this.$tags.val(r), this.$tags.focus()), this._overflowTagCheck();
                }
            },
            _focusAlbumTags: function() {
                var t = this.$tags.val();
