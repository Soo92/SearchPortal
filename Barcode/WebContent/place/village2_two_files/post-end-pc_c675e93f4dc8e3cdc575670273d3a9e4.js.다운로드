var videoIframeTag = function videoIframeTag(s, p, w, n, r) {
    var o = null;
    var v = 300;
    var x = 169;
    var m = null;
    var u = function(a) {
        return (!a || 0 === a.length || /^\s*$/.test(a))
    };
    var t = function(a) {
        if (typeof a === "string") {
            a = {
                value: a,
                outKeyMode: false
            }
        }
        return a
    };
    var m = t(p);
    if (u(s) || u(m.value)) { return null }
    if (u(w)) {
        w = v
    }
    if (u(n)) {
        n = x
    }
    var q = "<iframe frameborder='0' scrolling='no' width='" + w + "px' height='" + n
                    + "px' src='http://serviceapi.nmv.naver.com/view/ugcPlayer.nhn?vid=" + s;
    if (m.outKeyMode) {
        q += "&outKey=" + m.value
    } else {
        q += "&inKey=" + m.value
    }
    if (r != null) {
        if (r.copyrighter !== undefined && r.copyrighter != null && r.copyrighter != "") {
            q += "&copyrighter=" + r.copyrighter
        }
        if (r.playerType !== undefined && r.playerType != null && r.playerType != "") {
            q += "&pType=" + r.playerType
        }
        if (r.callbackDomain !== undefined && r.callbackDomain != null && r.callbackDomain != "") {
            q += "&callbackDomain=" + r.callbackDomain
        }
        if (r.infoDisplay !== undefined && r.infoDisplay != null) {
            q += "&infoDisplay=" + r.infoDisplay
        }
        if (r.hasThumbnails !== undefined && r.hasThumbnails != null) {
            q += "&hasThumbnails=" + (r.hasThumbnails === true ? "1" : "0")
        }
        if (r.hasLink !== undefined && r.hasLink != null) {
            q += "&hasLink=" + (r.hasLink === true ? "1" : "0")
        }
        if (r.hasFullScreen !== undefined && r.hasFullScreen != null) {
            q += "&hasFullScreen=" + (r.hasFullScreen === true ? "1" : "0")
        }
        if (r.hasLogo !== undefined && r.hasLogo != null) {
            q += "&hasLogo=" + (r.hasLogo === true ? "1" : "0")
        }
        if (r.sizeRatio !== undefined && r.sizeRatio != null) {
            q += "&sizeRatio=" + (r.sizeRatio === true ? "1" : "0")
        }
    }
    q += "' allowfullscreen></iframe>";
    return q
