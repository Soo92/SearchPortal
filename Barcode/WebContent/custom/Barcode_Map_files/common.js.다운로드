var BBS={goBBS:function(f,g,j){var i="";if(f!=null){i="?docId="+f}if(g!=null){PAGE.nClickForNoLink(g,j)}var h=topPath+"bbs/notice.nhn"+i;PAGE.goLink(h,"IN");return false}};var SERVICE={goService:function(i,f,h,g,j){PAGE.nClickForNoLink(g,j);if(f=="LINK01"){h=topPath+"service/main.nhn?serviceNo="+i;PAGE.goLink(h,"IN")}else{if(f=="LINK02"){PAGE.goLink(h,"IN")}else{if(f=="LINK03"){PAGE.goLink(h,"OUT")}}}return false}};var PAGE={goLink:function(g,e,f,h){if(f!=null){this.nClickForNoLink(f,h)}if(e==null){e="IN"}if(e=="IN"){location.href=g}else{if(e=="OUT"){window.open(g)}}return false},change:function(b){location.href=topPath+b;return false},nClickForNoLink:function(e,f){var d=new Object();clickcr(d,e,"","",f)}};var CATEGORY={change:function(l,s,o,m,n,t,r){if(r!=null){PAGE.nClickForNoLink(r,t)}else{if(n=="GRP01"){PAGE.nClickForNoLink("ctg.howlist",t)}else{if(n=="GRP02"){PAGE.nClickForNoLink("ctg.helplist",t)}}}if(o=="VC05"){window.open(m)}else{if(o=="VC06"){jQuery(location).attr("href",m)}else{var k=jQuery('<form id="moveToContents" style="display:none"></form>');var p=jQuery('<input type="hidden" name="serviceNo" value="'+l+'"/>');var q=jQuery('<input type="hidden" name="categoryNo" value="'+s+'"/>');k.append(p).append(q);jQuery("#content").append(k);k.prop("action",topPath+"contents/contents.nhn");if(o=="VC01"){k.prop("action",topPath+"service/main.nhn")}k.prop("method","get");k.submit()}}return false}};jQuery.fn.firstClickForInput=function(){var c=jQuery(this);var d=true;this.isNotFirstClicked=function(){return d};return this.each(function(){c.one("click",function(){d=false;jQuery(this).val("")})})};jQuery.fn.searchInitialSound=function(o){var k=this;var l=jQuery(this);var m=true;var i="";var n=-1;var j=-1;var p=["ㄱ","ㄲ","ㄴ","ㄷ","ㄸ","ㄹ","ㅁ","ㅂ","ㅃ","ㅅ","ㅆ","ㅇ","ㅈ","ㅉ","ㅊ","ㅋ","ㅌ","ㅍ","ㅎ"];this.openAutoKeyword=function(){o.show();l.addClass("fcs")};this.closeAutoKeyword=function(){o.hide();l.removeClass("fcs");n=-1;j=-1;o.find("li").removeClass("on")};this.makeServiceNameList=function(a){o.find("li").remove();jQuery.ajax({type:"POST",url:topPath+"json/getServiceGroup.nhn",data:{initialSound:a},dataType:"json",success:function(b){jQuery.each(b,function(c,d){o.find("ul").append(k.makeHtml(d))});k.linkEvent(o.find("li"));k.openAutoKeyword()}})};this.makeHtml=function(e){var c=e.serviceName;var f=e.serviceNo;var d=c.substring(0,1);var a=c.substr(1);var b='<li><a href="#" code="'+f+'" text="'+c+'"><strong>'+d+"</strong>"+a+"</a></li>";return b};this.setValue=function(a,b){l.val(a);l.attr("code",b)};this.linkEvent=function(c){var d=c.size()-1;var b;var a;l.unbind("keydown");l.keydown(function(e){if(e.keyCode==38){j=n;if(--n<0){n=d}b=jQuery(c[n]);a=b.find("a");jQuery(c[j]).removeClass("on");b.addClass("on");k.setValue(a.attr("text"),a.attr("code"))}else{if(e.keyCode==40){j=n;if(++n>d){n=0}b=jQuery(c[n]);a=b.find("a");jQuery(c[j]).removeClass("on");b.addClass("on");k.setValue(a.attr("text"),a.attr("code"))}}});c.mouseover(function(){jQuery(this).addClass("on")}).mouseout(function(){jQuery(this).removeClass("on")});c.find("a").click(function(){k.setValue($(this).attr("text"),$(this).attr("code"));k.closeAutoKeyword()})};this.getFirstSpelling=function(b){var a=b-44032;if(a>-1&&a<11172){result=p[Math.floor(a/588)]}else{result=String.fromCharCode(b)}return result};return this.each(function(){l.keyup(function(){var a=l.val();if(a.length>0){var b=k.getFirstSpelling(a.charCodeAt(0));if(i!=b){i=b;k.makeServiceNameList(i)}else{k.openAutoKeyword()}}else{i="";k.closeAutoKeyword()}}).focusout(function(){m=true}).click(function(){k.keyup();m=false});$(document).click(function(){if(m){k.closeAutoKeyword()}})})};function goTop(b){setTimeout(function(){jQuery.scrollTo(jQuery(b),100,{offset:-10})},100)};