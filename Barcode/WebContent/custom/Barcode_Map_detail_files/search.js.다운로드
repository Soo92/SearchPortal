function checkKeywordCut(j,m){var i=0;var h="";for(var n=0;n<j.length;n++){var l=j.charAt(n);var k=escape(l);if(k.length==1){i++}else{if(k.indexOf("%u")!=-1){i+=3}else{if(k.indexOf("%")!=-1){i+=k.length/3}}}if(i>m){break}else{h+=l}}return h}function checkKeywordSearchSize(i){var g=0;var h=i.substring(0);if(h==null){return 0}for(var f=0;f<h.length;f++){var j=escape(h.charAt(f));if(j.length==1){g++}else{if(j.indexOf("%u")!=-1){g+=3}else{if(j.indexOf("%")!=-1){g+=j.length/3}}}}return g};